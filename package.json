{
	"name": "keeb-mart",
	"version": "1.0.0",
	"description": "Mechanical keyboard marketplace",
	"main": "index.js",
	"scripts": {
		"client": "NODE_ENV=development webpack-dev-server --config build/webpack.config.js",
		"server": "NODE_ENV=development nodemon ./server/index.js",
		"start": "concurrently \"cross-env NODE_ENV=development webpack-dev-server --config build/webpack.config.js\" \"cross-env NODE_ENV=development nodemon ./server/index.js\"",
		"build": "cross-env NODE_ENV=production webpack --mode production --config build/webpack.config.js"
	},
	"dependencies": {
		"@babel/core": "^7.0.0",
		"@babel/polyfill": "^7.0.0",
		"@babel/preset-env": "^7.0.0",
		"@babel/preset-react": "^7.0.0",
		"axios": "^0.18.0",
		"babel-jest": "^22.4.1",
		"babel-loader": "^8.0.0",
		"babel-plugin-react-css-modules": "^5.2.1",
		"body-parser": "^1.18.3",
		"browserslist": "^4.2.1",
		"clean-webpack-plugin": "^0.1.17",
		"connected-react-router": "^4.3.0",
		"cookie-parser": "~1.3.5",
		"debug": "~2.2.0",
		"dotenv": "^2.0.0",
		"eslint-plugin-babel": "^5.1.0",
		"express": "^4.13.3",
		"express-subdomain": "^1.0.5",
		"morgan": "^1.9.1",
		"nodemailer": "^2.6.4",
		"react": "^16.4.0",
		"react-dom": "^16.4.0",
		"react-hot-loader": "^4.3.11",
		"react-router-dom": "^4.2.2",
		"request": "^2.79.0",
		"serve-favicon": "^2.5.0",
		"style-loader": "^0.19.0",
		"webpack": "^4.24.0",
		"webpack-cli": "^3.1.2",
		"webpack-dev-server": "^3.1.10"
	},
	"devDependencies": {
		"@babel/plugin-proposal-class-properties": "^7.1.0",
		"@babel/register": "^7.0.0",
		"babel-eslint": "^8.2.3",
		"concurrently": "^3.5.1",
		"cross-env": "^5.1.4",
		"css-loader": "^0.28.7",
		"eslint": "^4.19.0",
		"eslint-formatter-pretty": "^1.3.0",
		"eslint-loader": "^2.0.0",
		"eslint-plugin-react": "^7.7.0",
		"extract-text-webpack-plugin": "^4.0.0-beta.0",
		"file-loader": "^1.1.5",
		"html-webpack-plugin": "^4.0.0-beta.1",
		"ignore-styles": "^5.0.1",
		"jest": "^22.4.2",
		"less": "^3.0.1",
		"less-loader": "^4.1.0",
		"nodemon": "^1.18.5",
		"postcss-less": "^3.1.2",
		"react-test-renderer": "^16.2.0"
	},
	"jest": {
		"verbose": true
	},
	"babel": {
		"plugins": [
			"react-hot-loader/babel",
			"@babel/plugin-proposal-class-properties",
			"@babel/plugin-transform-async-to-generator"
		],
		"presets": [
			"@babel/preset-react",
			"@babel/env"
		]
	},
	"browserslist": "> 0.25%, not dead",
	"nodemonConfig": {
		"ignore": [
			"frontend/*",
			"docs/*",
			"_docs/*",
			"node_modules/*"
		],
		"delay": "2500"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Jlevyd15/keeb-mart.git"
	},
	"author": "Jeremy Levy",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/Jlevyd15/keeb-mart/issues"
	},
	"homepage": "https://github.com/Jlevyd15/keeb-mart#readme"
}
